// Place your key bindings in this file to overwrite the defaults
[
    // Toggle between terminal and editor focus â€“ https://stackoverflow.com/a/43012779/4970
    { "key": "cmd+'", "command": "workbench.action.terminal.focus"},
    { "key": "cmd+'", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus"},
    { "key": "shift+cmd+'", "command": "workbench.action.toggleMaximizedPanel"},

    {
        "key": "cmd+k h",
        "command": "workbench.action.focusLeftGroup"
    },
    {
        "key": "cmd+k l",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "cmd+k k",
        "command": "workbench.action.focusAboveGroup"
    },
    {
        "key": "cmd+k j",
        "command": "workbench.action.focusBelowGroup"
    },
    {
        "key": "shift+cmd+]",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+[",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k",
        "command": "-workbench.action.terminal.clear",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k j",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k l",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k k",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k h",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+backspace",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "alt+cmd+b e",
        "command": "workbench.view.extension.bookmarks"
    },
    {
        "key": "alt+cmd+b l",
        "command": "bookmarks.listFromAllFiles"
    },
    {
        "key": "alt+cmd+g e",
        "command": "workbench.view.extension.gitlens"
    },
    {
        "key": "ctrl+alt+up",
        "command": "editor.action.smartSelect.expand",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+down",
        "command": "editor.action.smartSelect.shrink",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+shift+8",
        "command": "editor.action.nextSelectionMatchFindAction",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+down",
        "command": "workbench.action.editor.nextChange",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+down",
        "command": "workbench.action.compareEditor.nextChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "ctrl+shift+up",
        "command": "workbench.action.editor.previousChange",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+up",
        "command": "workbench.action.compareEditor.previousChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "ctrl+shift+r",
        "command": "git.revertSelectedRanges"
    },
    {
        "key": "ctrl+shift+s",
        "command": "git.stageSelectedRanges"
    },
    {
        "key": "ctrl+shift+u",
        "command": "git.unstageSelectedRanges"
    },
    {
        "key": "shift+alt+s",
        "command": "cSpell.toggleEnableSpellChecker"
    },
    {
        "key": "cmd+k f",
        "command": "-workbench.action.closeFolder"
    },
    {
        "key": "shift+alt+cmd+j",
        "command": "editsHistory.moveCursorToNextEdit"
    },
    {
        "key": "cmd+k",
        "command": "-editsHistory.moveCursorToNextEdit"
    },
    {
        "key": "shift+alt+cmd+k",
        "command": "editsHistory.moveCursorToPreviousEdit"
    },
    {
        "key": "cmd+j",
        "command": "-editsHistory.moveCursorToPreviousEdit"
    },
    {
        "key": "alt+cmd+e",
        "command": "editsHistory.createEditAtCursor"
    },
    {
        "key": "cmd+u",
        "command": "-editsHistory.createEditAtCursor"
    },
    {
        "key": "shift+cmd+k",
        "command": "-editsHistory.moveCursorToNextEditedFile"
    },
    {
        "key": "shift+cmd+i",
        "command": "-editsHistory.moveCursorToNextEditInSameFile"
    },
    {
        "key": "shift+alt+cmd+e",
        "command": "editsHistory.removeEditsFromLine"
    },
    {
        "key": "cmd+i",
        "command": "-editsHistory.removeEditsFromLine"
    },
    {
        "key": "alt+cmd+p",
        "command": "editsHistory.moveCursorToPreviouslyEditedFile"
    },
    {
        "key": "shift+cmd+j",
        "command": "-editsHistory.moveCursorToPreviouslyEditedFile"
    },
    {
        "key": "alt+cmd+n",
        "command": "editsHistory.moveCursorToNextEditedFile"
    },
    {
        "key": "shift+cmd+u",
        "command": "-editsHistory.moveCursorToPreviousEditInSameFile"
    },
    {
        "key": "alt+cmd+b b",
        "command": "bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+k",
        "command": "-bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+l",
        "command": "-bookmarks.expandSelectionToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+j",
        "command": "-bookmarks.expandSelectionToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+k",
        "command": "-bookmarks.shrinkSelection",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+b j",
        "command": "bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+l",
        "command": "-bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+b k",
        "command": "bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+j",
        "command": "-bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+d",
        "command": "editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "cmd+d",
        "command": "-editor.action.addSelectionToNextFindMatch",
        "when": "editorFocus"
    },
    {
        "key": "alt+cmd+k",
        "command": "editsHistory.moveCursorToPreviousEditInSameFile"
    },
    {
        "key": "alt+cmd+j",
        "command": "editsHistory.moveCursorToNextEditInSameFile"
    },
    {
        "key": "alt+cmd+b alt+cmd+b",
        "command": "bookmarks.toggleLabeled"
    },
]
