// Place your key bindings in this file to overwrite the defaults
[
    // Toggle between terminal and editor focus â€“ https://stackoverflow.com/a/43012779/4970
    { "key": "cmd+'", "command": "workbench.action.terminal.focus"},
    { "key": "cmd+'", "command": "workbench.action.focusActiveEditorGroup", "when": "terminalFocus"},
    { "key": "shift+cmd+'", "command": "workbench.action.toggleMaximizedPanel"},

    {
        "key": "cmd+k h",
        "command": "workbench.action.focusLeftGroup"
    },
    {
        "key": "cmd+k l",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "cmd+k k",
        "command": "workbench.action.focusAboveGroup"
    },
    {
        "key": "cmd+k j",
        "command": "workbench.action.focusBelowGroup"
    },
    {
        "key": "shift+cmd+]",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "shift+cmd+[",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k",
        "command": "-workbench.action.terminal.clear",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k j",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k l",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k k",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+k h",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus"
    },
    {
        "key": "cmd+backspace",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
    },
    {
        "key": "alt+cmd+b e",
        "command": "workbench.view.extension.bookmarks"
    },
    {
        "key": "alt+cmd+b l",
        "command": "bookmarks.listFromAllFiles"
    },
    {
        "key": "alt+cmd+g e",
        "command": "workbench.view.extension.gitlens"
    },
    {
        "key": "ctrl+alt+up",
        "command": "editor.action.smartSelect.expand",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+down",
        "command": "editor.action.smartSelect.shrink",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+shift+8",
        "command": "editor.action.nextSelectionMatchFindAction",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+down",
        "command": "workbench.action.editor.nextChange",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+down",
        "command": "workbench.action.compareEditor.nextChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "ctrl+shift+up",
        "command": "workbench.action.editor.previousChange",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+up",
        "command": "workbench.action.compareEditor.previousChange",
        "when": "textCompareEditorVisible"
    },
    {
        "key": "ctrl+shift+r",
        "command": "git.revertSelectedRanges"
    },
    {
        "key": "ctrl+shift+s",
        "command": "git.stageSelectedRanges"
    },
    {
        "key": "ctrl+shift+u",
        "command": "git.unstageSelectedRanges"
    },
    {
        "key": "shift+alt+s",
        "command": "cSpell.toggleEnableSpellChecker"
    },
    {
        "key": "cmd+k f",
        "command": "-workbench.action.closeFolder"
    },
    {
        "key": "shift+alt+cmd+j",
        "command": "editsHistory.moveCursorToNextEdit"
    },
    {
        "key": "cmd+k",
        "command": "-editsHistory.moveCursorToNextEdit"
    },
    {
        "key": "shift+alt+cmd+k",
        "command": "editsHistory.moveCursorToPreviousEdit"
    },
    {
        "key": "cmd+j",
        "command": "-editsHistory.moveCursorToPreviousEdit"
    },
    {
        "key": "alt+cmd+e",
        "command": "editsHistory.createEditAtCursor"
    },
    {
        "key": "cmd+u",
        "command": "-editsHistory.createEditAtCursor"
    },
    {
        "key": "shift+cmd+k",
        "command": "-editsHistory.moveCursorToNextEditedFile"
    },
    {
        "key": "shift+cmd+i",
        "command": "-editsHistory.moveCursorToNextEditInSameFile"
    },
    {
        "key": "shift+alt+cmd+e",
        "command": "editsHistory.removeEditsFromLine"
    },
    {
        "key": "cmd+i",
        "command": "-editsHistory.removeEditsFromLine"
    },
    {
        "key": "shift+cmd+j",
        "command": "-editsHistory.moveCursorToPreviouslyEditedFile"
    },
    {
        "key": "shift+cmd+u",
        "command": "-editsHistory.moveCursorToPreviousEditInSameFile"
    },
    {
        "key": "alt+cmd+b b",
        "command": "bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+k",
        "command": "-bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+l",
        "command": "-bookmarks.expandSelectionToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+j",
        "command": "-bookmarks.expandSelectionToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+k",
        "command": "-bookmarks.shrinkSelection",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+b j",
        "command": "bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+l",
        "command": "-bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+b k",
        "command": "bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+j",
        "command": "-bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+cmd+k",
        "command": "editsHistory.moveCursorToPreviousEditInSameFile"
    },
    {
        "key": "alt+cmd+j",
        "command": "editsHistory.moveCursorToNextEditInSameFile"
    },
    {
        "key": "alt+cmd+b shift+b",
        "command": "bookmarks.toggleLabeled"
    },
    {
        "key": "ctrl+shift+n",
        "command": "-vscode-case-shifter.snakeCase",
        "when": "editorHasSelection"
    },
    {
        "key": "ctrl+shift+s",
        "command": "-vscode-case-shifter.studlyCase",
        "when": "editorHasSelection"
    },
    {
        "key": "ctrl+cmd+]",
        "command": "workbench.action.focusNextGroup"
    },
    {
        "key": "ctrl+cmd+[",
        "command": "workbench.action.focusPreviousGroup"
    },
    {
        "key": "shift+cmd+b",
        "command": "markdown.extension.editing.toggleBold",
        "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
    },
    {
        "key": "cmd+b",
        "command": "-markdown.extension.editing.toggleBold",
        "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+shift+cmd+down",
        "command": "search.action.focusNextSearchResult",
        "when": "hasSearchResult"
    },
    {
        "key": "f4",
        "command": "-search.action.focusNextSearchResult",
        "when": "hasSearchResult"
    },
    {
        "key": "ctrl+shift+cmd+up",
        "command": "search.action.focusPreviousSearchResult",
        "when": "hasSearchResult"
    },
    {
        "key": "shift+f4",
        "command": "-search.action.focusPreviousSearchResult",
        "when": "hasSearchResult"
    },
    {
        "key": "ctrl+cmd+'",
        "command": "workbench.action.toggleEditorWidths"
    },
    {
        "key": "ctrl+tab",
        "command": "workbench.action.quickOpenPreviousRecentlyUsedEditor",
        "when": "!inEditorsPicker"
    },
    {
        "key": "ctrl+shift+tab",
        "command": "workbench.action.quickOpenLeastRecentlyUsedEditor",
        "when": "!inEditorsPicker"
    },
    {
        "key": "alt+tab",
        "command": "workbench.action.openPreviousRecentlyUsedEditor",
        "when": "!inEditorsPicker"
    },
    {
        "key": "alt+shift+tab",
        "command": "workbench.action.openNextRecentlyUsedEditor",
        "when": "!inEditorsPicker"
    },
    {
        "key": "shift+cmd+c",
        "command": "-workbench.action.terminal.openNativeConsole",
        "when": "!terminalFocus"
    },
    {
        "key": "shift+alt+cmd+c",
        "command": "workbench.action.terminal.toggleFindCaseSensitive",
        "when": "terminalFindWidgetFocused"
    },
    {
        "key": "alt+cmd+c",
        "command": "-workbench.action.terminal.toggleFindCaseSensitive",
        "when": "terminalFindWidgetFocused"
    },
    {
        "key": "shift+alt+cmd+c",
        "command": "workbench.action.terminal.toggleFindCaseSensitive",
        "when": "terminalFocus"
    },
    {
        "key": "alt+cmd+c",
        "command": "-workbench.action.terminal.toggleFindCaseSensitive",
        "when": "terminalFocus"
    },
    {
        "key": "shift+alt+cmd+c",
        "command": "toggleFindCaseSensitive",
        "when": "editorFocus"
    },
    {
        "key": "alt+cmd+c",
        "command": "-toggleFindCaseSensitive",
        "when": "editorFocus"
    },
    {
        "key": "shift+alt+cmd+c",
        "command": "toggleSearchCaseSensitive",
        "when": "searchViewletFocus && searchViewletVisible && !fileMatchOrFolderMatchFocus"
    },
    {
        "key": "alt+cmd+c",
        "command": "-toggleSearchCaseSensitive",
        "when": "searchViewletFocus && searchViewletVisible && !fileMatchOrFolderMatchFocus"
    },
    {
        "key": "shift+alt+cmd+c",
        "command": "toggleSearchEditorCaseSensitive",
        "when": "inSearchEditor && searchInputBoxFocus"
    },
    {
        "key": "alt+cmd+c",
        "command": "-toggleSearchEditorCaseSensitive",
        "when": "inSearchEditor && searchInputBoxFocus"
    },
    {
        "key": "ctrl+cmd+up",
        "command": "workbench.action.moveEditorToAboveGroup",
        "when": "-terminalFocus"
    },
    {
        "key": "ctrl+cmd+down",
        "command": "workbench.action.moveEditorToBelowGroup",
        "when": "-terminalFocus"
    },
    {
        "key": "ctrl+shift+alt+down",
        "command": "merge-conflict.next",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+alt+up",
        "command": "merge-conflict.previous",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+cmd+u",
        "command": "-workbench.action.output.toggleOutput"
    },
    {
        "key": "shift+cmd+u",
        "command": "cursorRedo"
    },
    {
        "key": "alt+cmd+b alt+cmd+b",
        "command": "bookmarksExplorer.focus"
    },
    {
        "key": "ctrl+shift+pagedown",
        "command": "editor.action.marker.next",
        "when": "editorFocus"
    },
    {
        "key": "alt+f8",
        "command": "-editor.action.marker.next",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+pageup",
        "command": "editor.action.marker.prev",
        "when": "editorFocus"
    },
    {
        "key": "shift+alt+f8",
        "command": "-editor.action.marker.prev",
        "when": "editorFocus"
    },
    {
        "key": "cmd+e cmd+e",
        "command": "-sqltools.executeQuery",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+e cmd+e",
        "command": "sqltools.executeCurrentQuery"
    }
]
