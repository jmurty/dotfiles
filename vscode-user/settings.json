{
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "window.zoomLevel": 0,
    "editor.fontFamily": "'Source Code Pro',Menlo,'Fira Code',Monaco,'Courier New', monospace",
    "editor.fontSize": 12,
    "editor.fontWeight": "normal",
    "editor.lineHeight": 13,
    "editor.letterSpacing": 0.4,
    "editor.fontLigatures": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "editor.rulers": [80,100],
    "editor.minimap.enabled": false,
    "diffEditor.ignoreTrimWhitespace": true,
    "diffEditor.renderSideBySide": true,
    "diffEditor.originalEditable": true,
    "files.insertFinalNewline": true,
    "python.venvFolders": [
        "env",
        "venv",
        ".env",
        ".venv",
        ".pyenv",
        "./var/go.sh-venv",
    ],
    "git.autofetch": true,
    "gitlens.keymap": "chorded",
    "git.promptToSaveFilesBeforeCommit": true,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "html.format.wrapLineLength": 0,
    "python.jediEnabled": false,
    "editor.suggest.localityBonus": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.suggest.shareSuggestSelections": true,
    "terminal.integrated.cursorBlinking": true,
    "window.nativeFullScreen": false,
    "window.restoreFullscreen": true,
    "workbench.activityBar.visible": true,
    "git.showPushSuccessNotification": true,
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.saturation": 1,
    "material-icon-theme.opacity": 0.7,
    "editor.renderLineHighlight": "all",
    "files.hotExit": "onExitAndWindowClose",
    "workbench.commandPalette.preserveInput": true,
    "workbench.quickOpen.preserveInput": true,
    "window.restoreWindows": "all",
    "cSpell.language": "en-GB",
    "cSpell.userWords": [
        "Murty",
        "Redshift",
        "fallback",
        "falsey",
        "jmurty",
        "namedtuple",
        "pipenv",
        "postgre",
        "pyenv",
        "pytest",
        "sql",
        "traceback",
        "tuple",
        "virtualenv"
    ],
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/objects/**",
        ".vscode",
        "**/Application Support/**"
    ],
    "editor.multiCursorModifier": "alt",
    "debug.inlineValues": true,
    "clock.alignment": "Right",
    "clock.priority": 30,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "githubPullRequests.showInSCM": true,
    // Setting this to `true` breaks the SCM panel, see https://github.com/microsoft/vscode/issues/82203#issuecomment-547664774
    "scm.alwaysShowProviders": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.confirmOnExit": true,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.windowsEnableConpty": false,
    "editsHistory.showInformationMessages": true,
    "gotoNextPreviousMember.symbolKinds": [
        // "array",
        "boolean",
        "class",
        "constant",
        "constructor",
        "enum",
        "enummember",
        "event",
        "field",
        "file",
        "function",
        "interface",
        "key",
        "method",
        "module",
        "namespace",
        "null",
        "number",
        "object",
        "operator",
        "package",
        // "property", // Matches array comprehensions
        // "string",
        "struct",
        "typeparameter",
        // "variable"
    ],
    "files.eol": "\n",
    "files.exclude": {
        "/venv/**": true,
        "**/*.pyc": {
            "when": "$(basename).py"
        },
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "gitlens.settings.mode": "advanced",
    "editsHistory.maxHistory": 100,
    "quick-select.includeBackticks": false,
    "bookmarks.navigateThroughAllFiles": true,
    "bookmarks.saveBookmarksInProject": true,
    "sqltools.results": {
        "location": "active"
    },
    "python.workspaceSymbols.exclusionPatterns": [],
    "liveshare.languages.allowGuestCommandControl": true,
    "liveshare.showInStatusBar": "whileCollaborating",
    "editor.minimap.showSlider": "always",
    "editor.minimap.maxColumn": 80,
    "clock.format": "yyyy-mm-dd h:MMtt",
    "python.autoComplete.extraPaths": [
        "./src"
    ],
    "[markdown]": {
        "editor.defaultFormatter": "tehnix.vscode-tidymarkdown"
    },
    "markdown.extension.showExplorer": true,
    "markdown.extension.toc.githubCompatibility": true,
    "extensions.autoUpdate": false,
    "java.home": "/Library/Java/JavaVirtualMachines/jdk1.8.0_192.jdk/Contents/Home",
    "peacock.surpriseMeOnStartup": false,
    "editor.cursorSurroundingLines": 5,
    "kite.showWelcomeNotificationOnStartup": false,
    "gitlens.views.repositories.location": "gitlens",
    "gitlens.views.fileHistory.location": "gitlens",
    "gitlens.views.lineHistory.location": "gitlens",
    "gitlens.views.compare.location": "gitlens",
    "gitlens.views.search.location": "gitlens",
    "customizeUI.stylesheet": {
        // Hid excessive "editor action" buttons in editor tab bar
        ".editor-actions": "display: none !important;",
    },
    "rewrap.wrappingColumn": 79,
    "workbench.colorTheme": "Solarized Dark",
    "editor.minimap.scale": 2,
    "gitlens.views.repositories.branches.layout": "list",
    "git-graph.dateFormat": "Relative",
    "git-graph.defaultFileViewType": "File List",
    "git-graph.dialog.createBranch.checkOut": true,
    "git-graph.showStatusBarItem": false,
    "git-graph.showCurrentBranchByDefault": true,
    "gitlens.heatmap.ageThreshold": "21",
    "terminal.integrated.minimumContrastRatio": 4.5,
    "vsintellicode.features.python.deepLearning": "enabled",
    "search.smartCase": true,
    "search.enableSearchEditorPreview": true,
    "editor.smoothScrolling": true,
    "markdown.extension.toc.levels": "2..6",
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.memory.keepLibraryLocalVariables": true,
    "markdown.preview.fontSize": 12,
    "markdown.preview.lineHeight": 1
}
